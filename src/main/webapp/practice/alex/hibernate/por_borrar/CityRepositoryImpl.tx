package com.psg.objectboard.model.repository;

import com.psg.objectboard.model.EntityManagerHolder;

import javax.persistence.EntityManager;

public class CityRepositoryImpl {

    public CityEntity getById(Long id){
        EntityManager entityManager =new EntityManagerHolder().getCurrentEntityManager();
        CityEntity cityEntity= entityManager.find(CityEntity.class, id);
        System.out.println("CityEntityImpl Read");
        return cityEntity;
    }
}
